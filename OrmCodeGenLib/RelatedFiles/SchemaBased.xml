<?xml version="1.0" encoding="utf-8"?>
<oos:OrmObjects xmlns:oos="http://www.wise-orm.com/OrmObjectsSchema.xsd" namespace="XMedia.Framework.Media.Objects" schemaVersion="1">
	<oos:Entities>
		<oos:Entity id="eArtist" name="Artist">
			<oos:Tables>
				<oos:Table ref="tblArtists"/>
				<oos:Table ref="tblSiteAccess"/>
			</oos:Tables>
			<oos:Properties>
				<oos:Property id="id" propertyName="ID" attributes="PK" table="tblArtists" name="id" typeRef="tInt32"/>
				<oos:Property id="title" propertyName="Title" table="tblArtists" name="name" typeRef="tString"/>
			</oos:Properties>
		</oos:Entity>
		<oos:Entity id="eAlbum" name="Album">
			<oos:Tables>
				<oos:Table ref="tblAlbums"/>
				<oos:Table ref="tblSiteAccess"/>
			</oos:Tables>
			<oos:Properties>
				<oos:Property id="id" propertyName="ID" attributes="PK" table="tblAlbums" name="id" typeRef="tInt32"/>
				<oos:Property id="title" propertyName="Title" table="tblAlbums" name="title" typeRef="tString"/>
			</oos:Properties>
		</oos:Entity>
		<oos:Entity id="Album2ArtistRelation" name="Album2ArtistRelation">
			<oos:Tables>
				<oos:Table ref="tblAl2Ar"/>
			</oos:Tables>
			<oos:Properties>
				<oos:Property id="id" propertyName="ID" attributes="PK" table="tblAl2Ar" name="id" typeRef="tInt32" />
				<oos:Property id="album" propertyName="Album" table="tblAl2Ar" name="album_id" typeRef="tAlbum" />
				<oos:Property id="artist" propertyName="Artist" table="tblAl2Ar" name="artist_id" typeRef="tArtist"
				/>
			</oos:Properties>
		</oos:Entity>
	</oos:Entities>
	<oos:EntityRelations>
		<oos:Relation table="tblAl2Ar" underlyingEntity="Album2ArtistRelation">
			<oos:Left entity="eArtist" fieldName="artist_id" cascadeDelete="false" property="artist"/>
			<oos:Right entity="eAlbum" fieldName="album_id" cascadeDelete="true" property="album"/>
		</oos:Relation>
	</oos:EntityRelations>
	<oos:Tables>
		<oos:Table id="tblAlbums">dbo.albums</oos:Table>
		<oos:Table id="tblArtists">dbo.artists</oos:Table>
		<oos:Table id="tblAl2Ar">dbo.al2ar</oos:Table>
		<oos:Table id="tblSiteAccess">dbo.sites_access</oos:Table>
	</oos:Tables>
	<oos:Types>
		<oos:Type id="tAlbum">
			<oos:Entity ref="eAlbum"/>
		</oos:Type>
		<oos:Type id="tArtist">
			<oos:Entity ref="eArtist"/>
		</oos:Type>
		<oos:Type id="tInt32">
			<oos:ClrType name="System.Int32"/>
		</oos:Type>
		<oos:Type id="tString">
			<oos:ClrType name="System.String"/>
		</oos:Type>
	</oos:Types>
</oos:OrmObjects>
